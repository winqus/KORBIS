{
  "deno.enablePaths": ["supabase/functions"],
  "deno.lint": true,
  "deno.unstable": [
    "bare-node-builtins",
    "byonm",
    "sloppy-imports",
    "unsafe-proto",
    "webgpu",
    "broadcast-channel",
    "worker-options",
    "cron",
    "kv",
    "ffi",
    "fs",
    "http",
    "net"
  ],
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#34a975",
    "activityBar.background": "#34a975",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#e1cef0",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#34a975",
    "statusBar.background": "#28825a",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#34a975",
    "statusBarItem.remoteBackground": "#28825a",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#28825a",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#28825a99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#28825a",
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "modificationsIfAvailable"
}
